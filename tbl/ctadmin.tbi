InstallGlobalFunction( ConstructV4G, function( tbl, facttbl, aut )

    local fus, chars;

    fus:= First( tbl.ComputedClassFusions, fus -> fus.name = facttbl ).map;
    facttbl:= CharacterTableFromLibrary( facttbl );
